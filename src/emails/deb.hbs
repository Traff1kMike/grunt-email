                                                            {{else}}
                                                            <p>
                                                         {{/if}}  
                                                            <img src="{{../../url}}/{{image}}" alt="">
                                                         {{#if link}}
                                                            </a>
                                                            {{else}}
                                                            </p>
                                                         {{/if}}  
                                                         </div>
                                                         {{else}}
                                                         {{#if link}}
                                                            <table class="btn btn-primary">
                                                               <tr>
                                                                  <td>
                                                                     <a href="{{link}}" target="_blank">View Now</a>
                                                                  </td>
                                                               </tr>
                                                            </table>
