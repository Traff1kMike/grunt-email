                                                               <tr>
                                                                  <td>
                                                                     <a href="{{link}}" target="_blank">View Now</a>
                                                                  </td>
                                                               </tr>
                                                            </table>
                                                         {{/if}}  
                                                      {{/if}}                                                     
                                                   {{/each}}
                                                {{/with}}

                                                <h2>{{ default.soldtitle }}</h2>
